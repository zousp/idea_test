<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTDMapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.sp.mapper.UserMapper" >
    <insert id="insertUser" parameterType="com.sp.pojo.Users">
     insert into users(username,userage) values(#{username},#{userage})
    </insert>

    <select id="selectUserAll" resultType="com.sp.pojo.Users">
      select * from users
    </select>
</mapper>